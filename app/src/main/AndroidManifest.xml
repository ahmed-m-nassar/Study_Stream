<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.studystream">

    <application
        android:name=".Base.DataBase.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".JoinCourse.JoinCourseActivity"></activity>
        <activity
            android:name=".NewCourse.NewCourseActivity"
            android:label="@string/title_activity_new_course"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Courses.CoursesActivity" />
        <activity android:name=".SignUp.SignUpActivity" />
        <activity android:name=".SignIn.SignInActivity" />
        <activity android:name=".Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>